        align-items: center;
        justify-content: center;
        z-index: 1000;
      }

      .loading-message {
        font-size: 1.2rem;
        margin-right: 10px;
      }

      .loading-spinner {
        border: 4px solid rgba(0, 0, 0, 0.1);
        border-radius: 50%;
        border-top: 4px solid #151111;
        width: 30px;
        height: 30px;
        animation: spin 1s linear infinite;
      }

      @keyframes spin {
        0% {
