    <style>
      body {
        margin: 0;
        padding: 0;
        background-color: #000;
        font-family: "Outfit", sans-serif;
      }

      .slider {
        width: 100%;
        height: 80vh;
        margin: 0;
        margin-top: 15vh;
        padding: 0;
        overflow: hidden;
        position: relative;
      }

      .slide {
        width: 100%;
        height: 100%;
        display: flex;
        justify-content: center;
        flex-direction: column;
        align-items: center;
        position: relative;
        transition: transform 2s cubic-bezier(1, 0, 0, 1),
          opacity 3s cubic-bezier(0, 1, 1, 0);
        transform-origin: center center;
      }

      .slide img {
        width: 100%;
        height: 100%;
      }

      .slide h2 {
        z-index: 1;
        color: #000;
        position: absolute;
        font-size: 10vh;
        color: #fff;
      }

      .slide button {
        margin-top: 10px;
        cursor: pointer;
        background-color: #fff;
        position: absolute;
      }

      @media (max-width: 768px) {
        .slider {
          height: 40vh;
        }

        .top-slider #slide-2 button {
          margin-top: 40% !important;
          margin-left: 22% !important;
        }

        .top-slider #slide-3 button {
          margin-bottom: -110% !important;
          margin-left: 60% !important;
        }

        .top-slider #slide-4 button {
          margin-bottom: -110% !important;
          margin-left: 0% !important;
        }

        .top-slider #slide-5 button {
          margin-bottom: -100% !important;
          margin-left: 0%;
        }

        .top-slider button {
          display: none !important;
        }

        .perf-vid {
          height: auto !important;

        }

        .slider .prev {
          font-size: 5vh !important;
          left: 2vh !important;
        }

        .custom-item {
          display: flex;
          align-items: center;
          justify-content: center;
        }

        .custom-item img {
          width: 10vh !important;
        }
      }

      .bg .row {
        min-height: 400px;
      }

      .bg-image {

        height: 100% !important;
        background-size: cover;
        background-position: center;
        background-repeat: no-repeat;
      }

      .reel-item {
        display: flex;
        flex-direction: column;
        align-items: center;
        text-align: center;
        margin-bottom: 20px;
        border-radius: 10px;
        background-color: #fff;
        overflow: hidden;
        position: relative;
        height: 40vh;
      }

      .reel-item img {
        width: 100%;
        height: 100%;
        border-radius: 1vh;
        object-fit: contain;
      }

      .reel-caption {
        background-color: rgba(0, 0, 0, 0.7);
        border-radius: 0 0 1vh 1vh;
        padding: 10px;
        width: 100%;
        position: absolute;
        bottom: -1px;
        display: flex;
        justify-content: space-around;
        align-items: center;
      }

      .reel-caption button {
        color: #fff;
        margin-right: 5px;
      }

      .reel-caption button:hover {
        color: #fff;
      }

      .container-category {
        font-weight: 500;
        height: max-content;
        background-color: #000;
        padding: 20px 80px;
        display: flex;
        overflow: auto;
        flex-wrap: nowrap;
        justify-content: space-evenly;
        -webkit-font-smoothing: antialiased;
      }

      .card-wrap {
        margin: 10px;
        transform: perspective(800px);
        transform-style: preserve-3d;
        cursor: pointer;
      }
      .card-wrap:hover .card-info {
        transform: translateY(0);
      }
      .card-wrap:hover .card-info p {
        opacity: 1;
      }
      .card-wrap:hover .card-info,
      .card-wrap:hover .card-info p {
        transition: 0.6s cubic-bezier(0.23, 1, 0.32, 1);
      }
      .card-wrap:hover .card-info:after {
        transition: 5s cubic-bezier(0.23, 1, 0.32, 1);
        opacity: 1;
        transform: translateY(0);
      }
      .card-wrap:hover .card-bg {
        transition: 0.6s cubic-bezier(0.23, 1, 0.32, 1),
          opacity 5s cubic-bezier(0.23, 1, 0.32, 1);
        opacity: 0.8;
      }
      .card-wrap:hover .card {
        transition: 0.6s cubic-bezier(0.23, 1, 0.32, 1),
          box-shadow 2s cubic-bezier(0.23, 1, 0.32, 1);
        box-shadow: rgba(255, 255, 255, 0.2) 0 0 40px 5px, white 0 0 0 1px,
          rgba(0, 0, 0, 0.66) 0 30px 60px 0, inset #333 0 0 0 5px,
          inset white 0 0 0 6px;
      }

      .card {
        position: relative;
        flex: 0 0 240px;
        width: 240px;
        height: 320px;
        background-color: transparent;
        overflow: hidden;
        border-radius: 10px;
        box-shadow: rgba(0, 0, 0, 0.66) 0 30px 60px 0, inset #333 0 0 0 5px,
          inset rgba(255, 255, 255, 0.5) 0 0 0 6px;
        transition: 1s cubic-bezier(0.445, 0.05, 0.55, 0.95);
      }

      .card-bg {
        opacity: 0.5;
        position: absolute;
        top: 0px;
        left: 0px;
        width: 100%;
        height: 100%;
        padding: 20px;
        background-repeat: no-repeat;
        background-position: center;
        background-size: cover;
        transition: 1s cubic-bezier(0.445, 0.05, 0.55, 0.95),
          opacity 5s 1s cubic-bezier(0.445, 0.05, 0.55, 0.95);
        pointer-events: none;
      }

      .card-info {
        padding: 20px;
        position: absolute;
        bottom: 0;
        color: #fff;
        transform: translateY(40%);
        transition: 0.6s 1.6s cubic-bezier(0.215, 0.61, 0.355, 1);
      }
      .card-info p {
        opacity: 0;

        transition: 0.6s 1.6s cubic-bezier(0.215, 0.61, 0.355, 1);
      }
      .card-info * {
        position: relative;
        z-index: 1;
      }
      .card-info:after {
        content: "";
        position: absolute;
        top: 0;
        left: 0;
        z-index: 0;
        width: 100%;
        height: 100%;
        background-image: linear-gradient(
          to bottom,
          transparent 0%,
          rgba(0, 0, 0, 0.6) 100%
        );
        background-blend-mode: overlay;
        opacity: 0;
        transform: translateY(100%);
        transition: 5s 1s cubic-bezier(0.445, 0.05, 0.55, 0.95);
      }

      .card-info h2 {
        margin-top: -10vh;
        font-size: 36px;
        font-weight: 500;
      }

      .custom-container {
        font-size: 5vw;
        color: #fff;
        letter-spacing: 15px;
        color: var(--pr-color);
        text-transform: uppercase;
      }

      .custom-div {
        display: flex;
        font-weight: bolder;
        font-family: "Syne", sans-serif !important;
        justify-content: center;
        align-items: center;
        height: 30vh;
      }

      .custom-div span {
        display: inline-block;
      }

      @media (max-width: 767px) {
        .custom-carousel-container {
