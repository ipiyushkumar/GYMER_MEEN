      }

      .icon {
        width: 20vh;
        height: 20vh;
        position: relative;
        color: #ffffff;
      }

      .icon::before {
        content: "";
        position: absolute;
        z-index: 1;
        left: 0%;
        top: 0%;
        transform: translate(-50%, -50%);
      }

      .icon::after {
        content: "";
        position: absolute;
        left: 0%;
        top: 0%;
        width: 100%;
        height: 100%;
        box-sizing: border-box;
        border-radius: 50%;
        border-left: 1vh dashed;
        border-bottom: 1vh dashed;
        animation: rotate 3s linear infinite;
