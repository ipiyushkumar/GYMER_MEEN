      }

      .sale-container {
        width: 95vw;
        height: 60vh;
        background-color: #000 !important;
        position: relative;
        left: 0;
        top: 0;
        cursor: pointer;
        overflow: hidden;
        pointer-events: auto;
      }
      .center {
        height: 60vh;
